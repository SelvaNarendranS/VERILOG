#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6039ff82c2c0 .scope module, "and_gate_tb" "and_gate_tb" 2 3;
 .timescale 0 0;
v0x6039ff83ec00_0 .var "a", 0 0;
v0x6039ff83ecd0_0 .var "b", 0 0;
v0x6039ff83eda0_0 .net "c", 0 0, L_0x6039ff83eea0;  1 drivers
S_0x6039ff82c450 .scope module, "and1" "and_gate" 2 8, 3 3 0, S_0x6039ff82c2c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "c";
L_0x6039ff83eea0 .functor AND 1, v0x6039ff83ec00_0, v0x6039ff83ecd0_0, C4<1>, C4<1>;
v0x6039ff82c680_0 .net "a", 0 0, v0x6039ff83ec00_0;  1 drivers
v0x6039ff83ea20_0 .net "b", 0 0, v0x6039ff83ecd0_0;  1 drivers
v0x6039ff83eae0_0 .net "c", 0 0, L_0x6039ff83eea0;  alias, 1 drivers
    .scope S_0x6039ff82c2c0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6039ff83ec00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6039ff83ecd0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6039ff83ec00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6039ff83ecd0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6039ff83ec00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6039ff83ecd0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6039ff83ec00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6039ff83ecd0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x6039ff82c2c0;
T_1 ;
    %vpi_call 2 20 "$monitor", "time = %0t | a = %b | b = %b | c = %b", $time, v0x6039ff83ec00_0, v0x6039ff83ecd0_0, v0x6039ff83eda0_0 {0 0 0};
    %vpi_call 2 21 "$dumpfile", "and_gate_dumpfile.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and_gate_tb.v";
    "and_gate.v";
