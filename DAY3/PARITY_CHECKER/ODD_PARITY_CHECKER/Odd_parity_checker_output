#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ea0c2377200 .scope module, "odd_parity_checker_tb" "odd_parity_checker_tb" 2 5;
 .timescale -9 -12;
v0x5ea0c238b0e0_0 .var "a", 4 0;
v0x5ea0c238b1a0_0 .net "b", 0 0, L_0x5ea0c238b240;  1 drivers
E_0x5ea0c233ea50 .event anyedge, v0x5ea0c238afc0_0, v0x5ea0c238aec0_0;
S_0x5ea0c23773b0 .scope begin, "$ivl_for_loop0" "$ivl_for_loop0" 2 26, 2 26 0, S_0x5ea0c2377200;
 .timescale -9 -12;
v0x5ea0c2377590_0 .var/i "i", 31 0;
S_0x5ea0c238acc0 .scope module, "epc" "odd_parity_checker" 2 10, 3 3 0, S_0x5ea0c2377200;
 .timescale 0 0;
    .port_info 0 /INPUT 5 "a";
    .port_info 1 /OUTPUT 1 "b";
v0x5ea0c238aec0_0 .net "a", 4 0, v0x5ea0c238b0e0_0;  1 drivers
v0x5ea0c238afc0_0 .net "b", 0 0, L_0x5ea0c238b240;  alias, 1 drivers
L_0x5ea0c238b240 .reduce/xnor v0x5ea0c238b0e0_0;
    .scope S_0x5ea0c2377200;
T_0 ;
    %wait E_0x5ea0c233ea50;
    %load/vec4 v0x5ea0c238b1a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %vpi_call 2 14 "$monitor", "Time = %0t | a = %b | b = %b - ERROR", $time, v0x5ea0c238b0e0_0, v0x5ea0c238b1a0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 16 "$monitor", "Time = %0t | a = %b | b = %b - NO ERROR", $time, v0x5ea0c238b0e0_0, v0x5ea0c238b1a0_0 {0 0 0};
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5ea0c2377200;
T_1 ;
    %vpi_call 2 21 "$dumpfile", "odd_parity_checker_dump" {0 0 0};
    %vpi_call 2 22 "$dumpvars" {0 0 0};
    %fork t_1, S_0x5ea0c23773b0;
    %jmp t_0;
    .scope S_0x5ea0c23773b0;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5ea0c2377590_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5ea0c2377590_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x5ea0c2377590_0;
    %pad/s 5;
    %store/vec4 v0x5ea0c238b0e0_0, 0, 5;
    %delay 5000, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5ea0c2377590_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5ea0c2377590_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .scope S_0x5ea0c2377200;
t_0 %join;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
